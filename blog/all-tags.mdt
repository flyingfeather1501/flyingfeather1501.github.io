@(local-require json
                threading
                "tags.rkt")

@(define tags.json (build-path (find-system-path 'temp-dir) "tags.json"))
@(define all-tags
   (if (file-exists? tags.json)
     (jsexpr->tags (string->jsexpr (file->string tags.json)))
     '()))

# Tags

@(tags->comma-html all-tags)
